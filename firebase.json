{
  "hosting": {
    "public": "dist",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "/en-US/**",
        "destination": "/en-US/index.html"
      },
      {
        "source": "hi/**",
        "destination": "/hi/index.html"
      }
    ],
    "redirects": [
      {
        "source": "/",
        "destination": "/en-US",
        "type": 301
      }
    ]
  }
}
