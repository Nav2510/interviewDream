{
  "name": "interview-dream",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"npm run start:ng\" \"npm run graphql:codegen:watch\"",
    "start:ng": "ng serve --port=5000",
    "build": "ng build",
    "test": "ng test --code-coverage",
    "lint": "eslint -c .eslintrc.js --ext .ts ./src",
    "e2e": "ng e2e",
    "graphql:coverage": "graphql coverage",
    "graphql:validate": "graphql validate",
    "graphql:codegen": "graphql codegen",
    "graphql:codegen:watch": "graphql codegen --watch",
    "pipeline": "npm run lint && npm run graphql:codegen && npm run build"
  },
  "private": true,
  "dependencies": {
    "@angular-eslint/eslint-plugin": "^12.2.2",
    "@angular-eslint/template-parser": "^12.2.2",
    "@angular/animations": "~12.0.5",
    "@angular/cdk": "~12.0.5",
    "@angular/common": "~12.0.5",
    "@angular/compiler": "~12.0.5",
    "@angular/core": "~12.0.5",
    "@angular/forms": "~12.0.5",
    "@angular/material": "^12.0.5",
    "@angular/platform-browser": "~12.0.5",
    "@angular/platform-browser-dynamic": "~12.0.5",
    "@angular/router": "~12.0.5",
    "@apollo/client": "^3.0.0",
    "@babel/compat-data": "7.8.0",
    "apollo-angular": "^2.6.0",
    "eslint-plugin-import": "^2.23.4",
    "graphql": "^15.5.0",
    "immer": "^6.0.3",
    "rxjs": "~6.6.7",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.0.5",
    "@angular-eslint/eslint-plugin-template": "^12.2.2",
    "@angular/cli": "~12.0.5",
    "@angular/compiler-cli": "~12.0.5",
    "@angular/language-service": "~12.0.5",
    "@graphql-cli/codegen": "1.17.24",
    "@graphql-cli/coverage": "2.1.0",
    "@graphql-cli/validate": "2.1.0",
    "@graphql-codegen/cli": "1.21.5",
    "@graphql-codegen/introspection": "1.18.2",
    "@graphql-codegen/near-operation-file-preset": "^1.18.1",
    "@graphql-codegen/typescript": "1.22.1",
    "@graphql-codegen/typescript-apollo-angular": "2.3.4",
    "@graphql-codegen/typescript-operations": "1.18.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/eslint-plugin-tslint": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "codelyzer": "^6.0.0",
    "eslint": "^7.30.0",
    "eslint-plugin-jsdoc": "^35.4.3",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-react": "^7.24.0",
    "graphql-cli": "^4.1.0",
    "jasmine-core": "~3.6.0",
